{
    "name": "loro102/isocodesvalidation",
    "description": "Laravel 5 wrapper for the IsoCodes Validation library from ronanguilloux forked pixelpeter",
    "keywords": [
        "laravel",
        "iso",
        "isocode",
        "validation"
    ],
    "homepage": "https://github.com/loro102/laravel5-isocodes-validation",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "loro102",
            "email": "loro102@gmail.com",
            "homepage": "https://github.com/loro102/laravel5-isocodes-validation",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php" : "^7.0",
        "ronanguilloux/isocodes": "^2.1.1"
    },
    "autoload": {
        "psr-4": {
            "loro102\\IsoCodesValidation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "loro102\\IsoCodesValidation\\Test\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.0.0",
        "satooshi/php-coveralls": "^1.0",
        "laravel/laravel": "5.5.*"
    }

}